{
  "name": "test-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter for tests with Babel support",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "coverage": "istanbul cover _mocha -- --compilers js:babel-core/register test",
    "lint": "node_modules/standard/bin/cmd.js ./src/**/*.js ./test/**/*.js"
  },
  "author": "JD",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "chai": "^3.5.0",
    "istanbul": "^1.0.0-alpha.2",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "standard": "^8.5.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "lodash": "^3.10.1"
  }
}
